<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Salve Control - {% block title '' %}</title>
    <meta name="description" content="{% block description '' %}">
    <meta name="author" content="Salve Control">
    <meta name="copyright" content="Salve Control">
    {% if isNoIndex is not defined %}
      <meta name="robots" content="index, nofollow">
    {% else %}
      <meta name="robots" content="noindex">

    {% endif %}
    {% for rssItem in rss %}<link rel="alternate" type="application/rss+xml" title="{{ rssItem.name }}" href="{{ rssItem.url }}"/>
    {% endfor %}

    <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('favicon/apple-icon-57x57.png')}}">
    <link rel="apple-touch-icon" sizes="60x60" href="{{ asset('favicon/apple-icon-60x60.png')}}">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('favicon/apple-icon-72x72.png')}}">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('favicon/apple-icon-76x76.png')}}">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('favicon/apple-icon-144x144.png')}}">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('favicon/apple-icon-120x120.png')}}">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('favicon/apple-icon-144x144.png')}}">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('favicon/apple-icon-152x152.png')}}">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('favicon/apple-icon-180x180.png')}}">
    <link rel="icon" type="image/png" sizes="192x192"  href="{{ asset('favicon/favicon-256x256.png')}}">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('favicon/favicon-32x32.png')}}">
    <link rel="icon" type="image/png" sizes="96x96" href="{{ asset('favicon/favicon-96x96.png')}}">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('favicon/favicon-16x16.png')}}">
    <link rel="manifest" href="{{ path('app_pwa_manifest') }}">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{{ asset('favicon/ms-icon-144x144.png')}}">
    <meta name="theme-color" content="#ffffff">

    <meta property="og:type" content="business.business">
    <meta property="og:title" content="Shoko Cosplay - {{ og_title }}">
    <meta property="og:url" content="{{ absolute_url(path('app_root')) }}">
    <meta property="og:image:width" content="1200">
    <meta property="og:image:height" content="630">
    <meta property="og:image" content="{{ ogImage(og_image,'og_image') }}">
    <meta property="og:description" content="{{ og_description }}">

    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="Shoko Cosplay - {{ og_title }}">
    <meta name="twitter:description" content="{{ og_description }}">
    <meta name="twitter:image" content="{{ ogImage(og_image,'twitter_card') }}">


    <script type="application/ld+json">
      {
        "@context": "http://schema.org/",
        "@type": "WebSite",
        "url": "{{ absolute_url(path('app_root')) }}"
      }
    </script>
    <script type="application/ld+json">
      {
        "@context": "http://schema.org/",
        "@type": "Organization",
        "name": "Slave Control",
        "logo": "{{ ogImage(og_image,'webp') }}",
        "url": "{{ absolute_url(path('app_root')) }}",
        "address": {
          "@type": "PostalAddress",
          "addressCountry": "France"
        }
      }
    </script>
    <script type="application/ld+json">
      {
        "@context": "http://schema.org/",
        "@type": "Store",
        "name": "Slave Control",
        "image": "{{ ogImage(og_image,'webp') }}",
        "priceRange": "$",
        "url": "{{ absolute_url(path('app_root')) }}",
        "address": {
          "@type": "PostalAddress",
          "addressCountry": "France"
        }
     }
    </script>

    <link rel="preconnect" href="https://fonts.googleapis.com">

    {% if app.environment == "prod" %}
      <script defer data-api="/api/event" data-domain="slave.shoko-cosplay.fr" src="http://analytics.shoko-cosplay.fr/js/script.file-downloads.hash.outbound-links.pageview-props.revenue.tagged-events.js"></script>
    {% else %}
      <script defer data-domain="slave.shoko-cosplay.fr" src="http://esyweb.local:7070/js/script.file-downloads.hash.outbound-links.pageview-props.revenue.tagged-events.js"></script>
    {% endif %}
    <script>window.plausible = window.plausible || function() { (window.plausible.q = window.plausible.q || []).push(arguments) }</script>
    <script src="https://js.hcaptcha.com/1/api.js" async defer></script>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    {{ encore_entry_link_tags('app') }}
  </head>
  <body>
    <header>

    </header>
    <main>
      {% block content %}
      {% endblock %}
    </main>
    <footer>
    <div class="legal-line">
      <a href="{{ path('app_legal_mention') }}" class="legal-link">Mentions légale</a>
      <a href="{{ path('app_legal_rgpd') }}" class="legal-link">Donnés personnel</a>
      <a href="{{ path('app_legal_cgu') }}" class="legal-link">Conditions générales d'utilisation</a>
      <a href="{{ path('app_legal_contact') }}" class="legal-link">Contact</a>
      <a href="{{ path('app_legal_credit') }}" class="legal-link">Crédits</a>
      <a href="{{ path('app_legal_rules') }}" class="legal-link">Réglement</a>

    </div>
  </footer>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
  <script src="{{ asset('lib  s/js/jquery.min.js') }}" type="text/javascript"></script>
  <script src="https://kit.fontawesome.com/ed98241613.js" crossorigin="anonymous"></script>
  {{ encore_entry_script_tags('app') }}
  <script>
    window.shoko = {
      env: "{{ app.environment }}",
      sentry_dns: "{{ env.SENTRY_DSN_FRONTEND }}",
      version: "{{ env.VERSION }}",
      h_captcha: "{{ env.H_CAPTCHA }}"
    }
  </script>
  </body>
</html>
